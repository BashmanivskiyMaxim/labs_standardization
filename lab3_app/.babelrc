{
  "presets": [
    "@babel/env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "babel-plugin-transform-runtime",
    [
      "@babel/plugin-proposal-record-and-tuple",
      {
        "syntaxType": "hash"
      }
    ]
  ],
  "parserOpts": {
    "plugins": ["dynamicImport"]
  }
}
